{
    "name": "slidev-theme-iodigital",
    "description": "An iO Digital theme for Slidev",
    "version": "1.0.0",
    "author": {
        "name": "iO",
        "website": "https://iodigital.com"
    },
    "repository": "https://github.com/iodigital-com/slidev-theme-iodigital",
    "keywords": [
        "slidev-theme",
        "slidev"
    ],
    "engines": {
        "node": ">=14.0.0",
        "slidev": ">=0.19.3"
    },
    "main": "example.md",
    "scripts": {
        "build": "slidev build example.md",
        "dev": "slidev example.md --open",
        "export": "slidev export example.md",
        "screenshot": "slidev export example.md --format png",
        "lint": "eslint --fix --ext .js,.ts,.vue ."
    },
    "dependencies": {
        "@slidev/types": "^0.40.5",
        "codemirror-theme-vars": "^0.1.2",
        "prism-theme-vars": "^0.2.4",
        "theme-vitesse": "^0.6.4"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.6.1",
        "@commitlint/config-conventional": "^17.6.1",
        "@iconify-json/mdi": "^1.1.50",
        "@slidev/cli": "^0.40.5",
        "@typescript-eslint/eslint-plugin": "^5.59.2",
        "@typescript-eslint/parser": "^5.59.2",
        "eslint": "^8.39.0",
        "eslint-plugin-vue": "^9.11.0",
        "husky": "^8.0.3",
        "unplugin-auto-import": "^0.15.2"
    },
    "slidev": {
        "colorSchema": "light",
        "highlighter": "shiki",
        "defaults": {
        "favicon": "./logo/iO-logo_black.svg",
        "transition": "slide-left",
        "lineNumbers": true,
        "hightlighter": "shiki"
        }
    }
}
